<UserControl x:Class="MsOSProgram.ui.unit.Playground.Table.CpuVisualizationItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MsOSProgram.ui.unit.Playground.Table"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="400">
    <Grid>
        <Grid Grid.Row="0">
            <ScrollViewer HorizontalScrollBarVisibility="Visible"  VerticalScrollBarVisibility="Disabled" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <Grid  x:Name="gridCpuGraph" HorizontalAlignment="Stretch" Margin="0,0,30,0">
                </Grid>
            </ScrollViewer>
        </Grid>
    </Grid>
</UserControl>
